{
 "background": {"scripts": ["background.js"]},
 "browser_action": {
 "default_icon": "icon-128.png",
 "default_title": "LinkedIn Invite Opener"
 },
 "name": "LinkedIn Invite Opener",
 "description": "This little Chrome plugin makes it easy to reply to all outstanding LinkedIn invites. Made by Joost Plattel",
 "homepage_url": "http://jplattel.nl",
 "icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" },
 "permissions": [
     "tabs",
     "http://*/*",
     "https://*/*"
 ],
 "content_scripts": [ {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.2.3.min.js"]
} ],
 "version": "0.1",
 "manifest_version": 2
}